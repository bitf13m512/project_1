    <script type="text/javascript">
        window.history.forward();
        function noBack() { window.history.forward(); }
    </script>


</head>

<body background="../../Content/night.jpg" onload="noBack();"
      onpageshow="if (event.persisted) noBack();" onunload="">
    <span style="font-size:40px;cursor:pointer; font-colour:white" onclick="openNav()"><h2 align="left">☰ open</h2></span>

    <style>
        body {
            font-family: "Lato", sans-serif;
        }

        .sidenav {
            height: 100%;
            width: 0;
            position: fixed;
            z-index: 1;
            top: 0;
            left: 0;
            background-color: #111;
            overflow-x: hidden;
            transition: 0.5s;
            padding-top: 60px;
        }

            .sidenav a {
                padding: 8px 8px 8px 32px;
                text-decoration: none;
                font-size: 25px;
                color: #818181;
                display: block;
                transition: 0.3s;
            }

                .sidenav a:hover, .offcanvas a:focus {
                    color: #f1f1f1;
                }

        .closebtn {
            position: absolute;
            top: 0;
            right: 25px;
            font-size: 36px !important;
            margin-left: 50px;
        }

        media screen and (max-height: 450px) {
            .sidenav;

        {
            padding-top: 15px;
        }

        .sidenav a {
            font-size: 18px;
        }

        }
    </style>

    <body>

        <div id="mySidenav" class="sidenav">
            <input type='file' onchange="readURL(this);" />

            <img id="blah" src="../../Content/b7.jpg" alt="your image" />
            <a href="javascript:void(0)" class="closebtn" onclick="closeNav()">×</a>
            <a href="../../admin/function">HOME</a>
            <a href="../../admin/add">ADD</a>
            <a href="../../admin/update">UPDATE</a>
            <a href="../../admin/delete">DELETE</a>
            <a href="../../admin/view">VIEW</a>
            <a href="../../admin/signup">SIGNUP</a>
        </div>


        @*<h2> <p>Click on the element below to open the side navigation menu.</p></h2>
            <span style="font-size:30px;cursor:pointer" onclick="openNav()"><h2>☰ open</h2></span>*@

        <script>
            function openNav() {
                document.getElementById("mySidenav").style.width = "250px";
            }

            function closeNav() {
                document.getElementById("mySidenav").style.width = "0";
            }
        </script>


        <div class="search1">
            <body>
                <div class="main">

                    <div class="content">
                        <div id="container">
                            <form action="../admin/signup1" method="post">
                                <div class="form_row">
                                    <label class="contact"><strong>USERNAME:</strong></label>
                                    <input type="text" name="name" id="n1" required /><div id="text"></div>
                                </div>
                                <button type="button" class="register" value="register" onclick="check()">check</button>
                                <div class="form_row">
                                    <label class="contact"><strong>New_Password:</strong></label>
                                    <input type="password" name="pass" required /><br />
                                </div>

                                <div class="form_row">
                                    <br />
                                    <input type="submit" class="register" value="submit" />
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </body>
